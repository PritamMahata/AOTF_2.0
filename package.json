{
  "name": "academy-of-tutorials-and-freelancers",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "dev": "pnpm --parallel -r --filter \"!@aotf/lib\" --filter \"!@aotf/models\" --filter \"!@aotf/utils\" dev",
    "dev:all": "pnpm --parallel -r --filter \"!@aotf/lib\" --filter \"!@aotf/models\" --filter \"!@aotf/utils\" dev",
    "dev:main": "pnpm --filter @aotf/main dev",
    "dev:admin": "pnpm --filter @aotf/admin dev",
    "dev:tutorials": "pnpm --filter @aotf/tutorials dev",
    "dev:jobs": "pnpm --filter @aotf/jobs dev",
    "build": "pnpm -r --filter \"!@aotf/lib\" --filter \"!@aotf/models\" --filter \"!@aotf/utils\" build",
    "build:main": "pnpm --filter @aotf/main build",
    "build:admin": "pnpm --filter @aotf/admin build",
    "build:tutorials": "pnpm --filter @aotf/tutorials build",
    "build:jobs": "pnpm --filter @aotf/jobs build",
    "start": "pnpm -r --filter \"!@aotf/lib\" --filter \"!@aotf/models\" --filter \"!@aotf/utils\" start",
    "start:main": "pnpm --filter @aotf/main start",
    "start:admin": "pnpm --filter @aotf/admin start",
    "start:tutorials": "pnpm --filter @aotf/tutorials start",
    "start:jobs": "pnpm --filter @aotf/jobs start",
    "lint": "pnpm -r --filter \"!@aotf/lib\" --filter \"!@aotf/models\" --filter \"!@aotf/utils\" lint",
    "seed:admin": "tsx src/scripts/seed-admin.ts",
    "update:admin-permissions": "tsx src/scripts/update-admin-permissions.ts",
    "update:admin-permissions-auto": "tsx src/scripts/update-admin-permissions-auto.ts",
    "clean": "pnpm store prune && rimraf node_modules && rimraf .next && rimraf pnpm-lock.yaml",
    "reinstall": "pnpm clean && pnpm install"
  },
  "dependencies": {
    "@dicebear/collection": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/themes": "^3.2.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.24",
    "geist": "^1.4.2",
    "gsap": "^3.13.0",
    "html-to-image": "^1.11.13",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.540.0",
    "mongoose": "^8.19.2",
    "nanoid": "^5.1.5",
    "next": "^15.4.6",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.6",
    "qrcode": "^1.5.4",
    "radix-ui": "^1.4.3",
    "razorpay": "^2.9.6",
    "react": "19.1.0",
    "react-avatar": "link:@@radix-ui/react-avatar",
    "react-day-picker": "^9.11.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.1.2",
    "resend": "^6.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
